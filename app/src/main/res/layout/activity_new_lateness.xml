<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:mapbox="http://schemas.android.com/apk/res-auto"
    tools:context="com.uca.devceargo.internic.activities.NewLateness">

    <com.mapbox.mapboxsdk.maps.MapView
        android:id="@+id/newsMapView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        mapbox:mapbox_cameraTargetLat="12.509359"
        mapbox:mapbox_cameraTargetLng="-84.617782"
        mapbox:mapbox_cameraZoom="6"
        mapbox:mapbox_styleUrl="mapbox://styles/mapbox/streets-v10" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/new_lateness_next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="end|bottom"
        android:layout_margin="16dp"
        android:src="@drawable/ic_done"
        />

</FrameLayout>
